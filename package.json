{
  "name": "yunmeng-editor",
  "version": "0.0.1",
  "scripts": {
    "dev:example": "npm -C example run dev",
    "build": "cross-env vite build -m rightMenu",
    "release": "sh ./scripts/release.sh",
    "prepare": "husky install",
    "lint:fix": "eslint \"{packages,example}/**/*.{vue,js,ts,tsx}\" --cache --fix"
  },
  "standard-version": {
    "changelogHeader": "# Changelog\n\n## Right-Menu更新日志如下: \n"
  },
  "type": "module",
  "description": "编辑器",
  "author": "ggbeng",
  "main": "./lib/index.umd.cjs",
  "module": "./lib/index.esm.js",
  "types": "./lib/package/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.esm.js",
      "require": "./lib/index.umd.js",
      "types": "./lib/package/index.d.ts"
    }
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "lodash-es": "^4.17.21",
    "vue": "^3.4.31"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.3",
    "@types/node": "^20.14.10",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "cross-env": "^7.0.3",
    "element-plus": "^2.7.7",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.27.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "picocolors": "^1.0.1",
    "prettier": "^3.3.2",
    "sass": "^1.77.7",
    "select-version-cli": "^0.0.2",
    "standard-version": "^9.5.0",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vite-plugin-dts": "^3.9.1"
  }
}
